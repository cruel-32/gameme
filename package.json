{
  "name": "gatsby-starter-ts",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <oi@joaopedro.cc> (joaopedro.cc)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "babel-plugin-styled-components": "^1.11.1",
    "classnames": "^2.2.6",
    "delay": "^4.4.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "gatsby": "^2.24.85",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-styled-components": "^3.3.14",
    "i18next": "^19.8.2",
    "immer": "^7.0.14",
    "konva": "^7.1.4",
    "moment": "^2.29.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "styled-components": "^5.2.0",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "use-sound": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/classnames": "^2.2.10",
    "@types/material-ui": "^0.21.8",
    "@types/node": "14.11.8",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "babel-plugin-root-import": "6.5.0",
    "commitizen": "4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "lint-staged": "10.4.2",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
